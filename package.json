{
  "name": "hiden-note",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "jest",
    "clearJestCache": "jest --clearCache",
    "setup:emu:firestore": "firebase setup:emulators:firestore",
    "emulator-start": "firebase emulators:start --only firestore",
    "test-watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/spec/**/*.test.ts"
    ]
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.2",
    "@nuxtjs/composition-api": "^0.17.0",
    "@nuxtjs/markdownit": "^1.2.10",
    "algoliasearch": "^4.8.5",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.9.0",
    "diff": "^5.0.0",
    "dotenv": "^8.2.0",
    "firebase": "^8.2.9",
    "firebaseui": "^4.7.3",
    "luxon": "^1.26.0",
    "markdown-it-highlightjs": "^3.4.0",
    "nuxt": "^2.15.2",
    "nuxt-fontawesome": "^0.4.0",
    "nuxt-property-decorator": "^2.9.1",
    "vuex-module-decorators": "^1.0.1",
    "vuexfire": "^3.2.5"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@nuxt/types": "^2.15.2",
    "@nuxt/typescript-build": "^2.0.6",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/algoliasearch": "^3.34.11",
    "@types/diff": "^4.0.2",
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fibers": "^5.0.0",
    "jest": "^26.6.3",
    "mocha": "^8.3.0",
    "prettier": "^2.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2"
  }
}
